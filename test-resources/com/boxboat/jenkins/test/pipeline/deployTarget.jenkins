@Library('jenkins-shared-library@master')
import com.boxboat.jenkins.pipeline.deploy.*


def execute() {

    params.deployTargetKey = "dev01"
    def deploy = new BoxDeploy()

    node() {
        deploy.wrap {
            stage('Deploy'){
                deploy.withCredentials() {
                    sh "helm upgrade --install test ."
                }
            }
        }
    }

}

return this
